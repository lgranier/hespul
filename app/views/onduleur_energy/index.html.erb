<%= search_form_for @search do |f| %>
  <%=  f.label 'Date' %>
  <%= f.search_field :date_gteq, class: 'form-control input-sm', 'datepicker' => true %>
  <%= f.search_field :date_lteq, class: 'form-control input-sm', 'datepicker' => true %>
<% end %>
<%= flash[:notice] %>
<table>
	<thead>
		<tr>
			<th>Onduleur</th>
			<th>date</th>
			<th>Energy</th>
		</tr>
	</thead>
	<tbody>
		<% @onduleurEnergies.each do |onduleur_energy| %>
		<tr>
			<td><%= onduleur_energy.onduleur_id %></td>
			<td><%= onduleur_energy.date %></td>
			<td><%= onduleur_energy.energy %></td>
		</tr>
		<% end %>
	</tbody>
</table>
<div>
	<h3>Import a CSV File</h3>
	<%= form_tag import_onduleur_energy_index_path, multipart: true do %>
	<%= file_field_tag :file %>
	<%= submit_tag "Import CSV" %>
	<% end %>
</div>
